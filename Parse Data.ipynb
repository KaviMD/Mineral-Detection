    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "data = pd.read_csv(\"data_files/CameraLocs.csv\",index_col=0)\n",
    "scale_columns = ['x','y','z']\n",
    "for col in scale_columns:\n",
    "    data[col] = data[col] * 10\n",
    "data['xr'] = data['xr'] * 0\n",
    "data.to_csv('data_files/ScaledLocs.csv')\n",
    "data.head(6)"
   ]
